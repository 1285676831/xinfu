<?xml version='1.0' encoding='UTF-8'?>
<config nodb="flase">

	<summary>
		入住管理
	</summary>
	
	<log>false</log>

	<set-request-attribute id="header.title" value="系统应用管理" />
    <set-request-attribute id="formTitleAddNew" value='新增系统应用'/>
    <set-request-attribute id="formTitleEdit" value='修改系统应用'/>
    <set-request-attribute id="formTitleFilter" value='查询系统应用'/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-ptdef.sql" source="sql" scope="transaction" />
		<recordset id="query-teacher.sql" source="sql" scope="transaction" /> 
	</transaction>
	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<expiration>0</expiration>
		<print mode="table" recordset="query-ptdef.sql" tag="ptdef-list"/>
		<print mode="table" recordset="query-teacher.sql" tag="teacher-list"/> 
	</output>

</config>
