<?xml version='1.0' encoding='UTF-8'?>

<config>

	<summary>
		Exportar documento excel directo al browser
	</summary>
	
	<log>false</log>
	<colname>daochu_end_date,daochu_name,daochu_start_date,daochu_code,
	daochu_group,daochu_status,daochu_relatecode,daochu_contracttype,daochu_statusall</colname>
	<sql-template>query-base.sql</sql-template>

	<transaction>
		<classname>transactions.project.fitness.SearchData</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>

	<excel recordset="query-base.sql" sheetname="租柜合同列表" filename="租柜合同列表.xls" header-background="true">
		<col id="code" label="合同编号" />
		<col id="contracttype" label="合同类型"/>
		<col id="i_status" label="状态" />
		<col id="name" label="姓名" />
		<col id="mobile" label="手机号" />
		<col id="net_code" label="柜子编号" />
		<col id="factmoney" label="实收金额" />
		<col id="net_start" label="租用开始日期" />
		<col id="net_end" label="租用截止日期" />
		<col id="salemember" label="销售员1" />
		<col id="salemember1" label="销售员2" />
		
	</excel>
</config>
