<?xml version='1.0' encoding='UTF-8'?>
<config nodb="false">

	<summary>
	</summary>
	
	<log>false</log>

    <transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>	
	    <recordset id="query-memberhead.sql" source="sql" scope="transaction"/>
	    <recordset id="query-skillscopenum.sql" source="sql" scope="transaction"/>
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode="form" recordset="query-memberhead.sql" />
		<print mode="form" recordset="query-skillscopenum.sql" />
	</output>

</config>
