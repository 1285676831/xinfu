<?xml version='1.0' encoding='UTF-8'?>
<config>

	<summary>
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-custnum.sql" source="sql" scope="transaction" />
		<recordset id="query-guestnum.sql" source="sql" scope="transaction" />
		<recordset id="query-inleft.sql" source="sql" scope="transaction" />
		<recordset id="query-inleftmean.sql" source="sql" scope="transaction" />
	</transaction>
	
	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>script.js</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/javascript</content-type>
		<expiration>0</expiration>
		<print mode="form" recordset="query-custnum.sql"/>
		<print mode="form" recordset="query-guestnum.sql"/>
		<print mode="form" recordset="query-inleft.sql"/>
		<print mode="form" recordset="query-inleftmean.sql"/>
	</output>

</config>
